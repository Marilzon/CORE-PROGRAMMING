{
	"compilerOptions": {
		"lib": [
			"ES2015"
		],
		"target": "ES2015",
		"module": "CommonJS",
		"strict": true,
		"outDir": "dist",
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts"
	]
}
